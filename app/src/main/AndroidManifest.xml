<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tlunet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TLUNet">
        <activity android:name=".ui.forgotPassword.ForgotPasswordActivity"></activity>
        <activity android:name=".ui.category.categorydetail.CategoryDetailActivity" />
        <activity android:name=".ui.category.CategoryActivity" />
        <activity android:name=".ui.home.HomeActivity" />
        <activity android:name=".ui.createaccount.CreateAccountActivity" />
        <activity android:name=".ui.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>